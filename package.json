{
  "name": "react-component-gen",
  "displayName": "React Component gen",
  "description": "React Component generator with jest test file",
  "publisher": "rsk88",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.genComponent"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.genComponent",
        "title": "Generate React Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.genComponent",
          "group": "navigation"
        }
      ]
    }
  },
  "repository": {
    "url": "https://github.com/revathskumar/react-component-gen"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "change-case": "^3.0.2",
    "handlebars": "^4.3.0"
  },
  "license": "MIT"
}
